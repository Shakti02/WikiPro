{
    "manifest_version": 2,
    "name": "Wikipro",
    "version": "1.0",
    "description": "Search and Speak Wikipedia on the go!",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions":[
        "tabs",
        "contextMenus",
        "contextMenus",
        "tts"
    ]
}
